<% include header %>
    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <% include navbar-header %>
            <!-- /.navbar-header -->
            <% include navbar-side %>
            <!-- /.navbar-static-side -->
        </nav>

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header"><%= service.name %></h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <% for(var i=0; i<charts.length; i++) { %>
                <div class="col-lg-6">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <%= charts[i].label %>
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div id="<%= charts[i].id %>"></div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <% } %>
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->
    <script src="/data/service/<%= service.id %>?chartType=<%= chartType %>"></script>
<% include footer %>